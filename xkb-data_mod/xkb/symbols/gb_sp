default
partial alphanumeric_keys
xkb_symbols "colemak_dh" {

    //  Alphanumeric section (letter keys and semicolon)
    key <AD01> { [             q,             Q ] }; // QWE Q / DH Q
    key <AD02> { [             w,             W ] }; // QWE W / DH W
    key <AD03> { [             f,             F ] }; // QWE E / DH F
    key <AD04> { [             p,             P ] }; // QWE R / DH P
    key <AD05> { [             b,             B ] }; // QWE T / DH B
    key <AD06> { [             j,             J ] }; // QWE Y / DH J
    key <AD07> { [             l,             L ] }; // QWE U / DH L
    key <AD08> { [             u,             U ] }; // QWE I / DH U
    key <AD09> { [             y,             Y ] }; // QWE O / DH Y
    key <AD10> { [     semicolon,         colon ] }; // QWE P / DH ;:
    //  U2039/203A single guillemets; U0283/01A9 esh/ESH
    key <AC01> { [             a,             A ] }; // QWE A / DH A
    key <AC02> { [             r,             R ] }; // QWE S / DH R
    key <AC03> { [             s,             S ] }; // QWE D / DH S
    key <AC04> { [             t,             T ] }; // QWE F / DH T
    key <AC05> { [             g,             G ] }; // QWE G / DH G
    key <AC06> { [             m,             M ] }; // QWE H / DH M
    key <AC07> { [             n,             N ] }; // QWE J / DH N
    key <AC08> { [             e,             E ] }; // QWE K / DH E
    key <AC09> { [             i,             I ] }; // QWE L / DH I
    key <AC10> { [             o,             O ] }; // QWE ; / DH O
    //
    key <LSGT> { [             z,             Z ] }; // QWE ISO / DH Z
    key <AB01> { [             x,             X ] }; // QWE Z / DH X
    key <AB02> { [             c,             C ] }; // QWE X / DH C
    key <AB03> { [             d,             D ] }; // QWE C / DH D
    key <AB04> { [             v,             V ] }; // QWE V / DH V
    key <AB05> { [     backslash,           bar ] }; // QWE B / DH ISO
    key <AB06> { [             k,             K ] }; // QWE N / DH K
    key <AB07> { [             h,             H ] }; // QWE M / DH H
    key <AB08> { [         comma,          less ] }; // ,
    key <AB09> { [        period,       greater ] }; // .
    key <AB10> { [         slash,      question ] }; // /?

    //  Alphanumeric section (number/symbol keys)
    key <TLDE> { [         grave,       notsign ] }; // `~
    key <AE01> { [             1,        exclam ] }; // 1
    key <AE02> { [             2,      quotedbl ] }; // 2
    key <AE03> { [             3,      sterling ] }; // 3
    key <AE04> { [             4,        dollar ] }; // 4
    key <AE05> { [             5,       percent ] }; // 5
    key <AE06> { [             6,   asciicircum ] }; // 6
    key <AE07> { [             7,     ampersand ] }; // 7
    key <AE08> { [             8,      asterisk ] }; // 8
    key <AE09> { [             9,     parenleft ] }; // 9
    key <AE10> { [             0,    parenright ] }; // 0
    key <AE11> { [         minus,    underscore ] }; // -_
    key <AE12> { [         equal,          plus ] }; // =+
    //
    key <AD11> { [   bracketleft,     braceleft ] }; // [{
    key <AD12> { [  bracketright,    braceright ] }; // ]}
    //
    key <AC11> { [    apostrophe,            at ] }; // '@
    key <BKSL> { [    numbersign,    asciitilde ] }; // #~
    //
    key <SPCE> { [         space,         space ] };

    include "gb_sp(symbols)"

    //include "level3(rwin_switch)"
    include "level3(ralt_switch)"

    name[Group1]= "English (UK Colemak-DH)";

};

partial alphanumeric_keys
xkb_symbols "colemak_dhk" {
    key <AC06> { [             k,             K ] }; // QWE H / DHk K
    key <AB06> { [             m,             M ] }; // QWE N / DHk M
    
    augment "gb_sp(colemak_dh)"

    name[Group1]= "English (UK Colemak-DHk)";
};

partial alphanumeric_keys
xkb_symbols "colemak_dh_shiftz" {
    key <LFSH> { [             z,             Z ] };

    augment "gb_sp(colemak_dh)"

    name[Group1]= "English (UK Colemak-DH ShiftZ)";
};

partial alphanumeric_keys
xkb_symbols "colemak_dhk_shiftz" {
    key <LFSH> { [             z,             Z ] };

    augment "gb_sp(colemak_dhk)"

    name[Group1]= "English (UK Colemak-DHk ShiftZ)";
};

